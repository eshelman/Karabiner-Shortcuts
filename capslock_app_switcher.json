{
  "title": "Caps Lock -> Cmd+Tab (app switcher)",
  "rules": [
    {
      "description": "Caps Lock acts as Cmd+Tab to open the macOS app switcher",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "caps_lock", "modifiers": { "optional": ["any"] } },
          "to": [
            { "key_code": "tab", "modifiers": ["command"] }
          ]
        }
      ]
    }
  ]
}
